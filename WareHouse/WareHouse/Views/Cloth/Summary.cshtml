@model System.Collections.Generic.List<WareHouse.Models.Cloth>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
</head>
<body>
    <h1>Green Fox Fashion Web Store</h1>
    <div>
        <table style="width:100%">
            <tr>
                <th>Item Name</th>
                <th>Manufacturer</th>
                <th>Category</th>
                <th>Quantity</th>
                <th>Sub-total price</th>
            </tr>

            @foreach (var cloth in Model)
            {
                <tr>
                    <td>@cloth.ItemName</td>
                    <td>@cloth.Manufacturer</td>
                    <td>@cloth.Category</td>
                    <td>@cloth.Size</td>
                    <td>@cloth.UnitPrice</td>
                </tr>
            }

        </table>
    </div>

    <h1><a href="/warehouse">Back to main</a></h1>
</body>
</html>